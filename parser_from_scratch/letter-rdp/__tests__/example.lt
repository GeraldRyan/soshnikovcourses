// parent class
class Point {
    def constructor(x,y){
        this.x = x;
        this.y = y;
    }
    
    def calc() {
        return this.x + this.y;
    }
}

// child class
class Point3D extends Point {
    def constructor(x, y, z) {
        super(x, y);
        this.z = z;
    }

    def calc() {
        return super() + this.z;
    }
}

// instance
new Point3D(10, 20, 30);